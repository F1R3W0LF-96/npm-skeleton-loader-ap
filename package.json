{
  "name": "skeleton-loader-ap",
  "version": "0.1.3",
   "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
 "files": [
  "dist",
  "dist/globals.css"
],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "tsup src/app/components/index.ts --dts --format esm,cjs --external react --external react-dom && cp src/app/globals.css dist/globals.css",
    "start": "next start",
    "lint": "next lint"
  },
"exports": {
  "./package.json": "./package.json",
  ".": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "./globals.css": "./dist/globals.css"
},
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "typescript": "^5"
  }
}
